  "name": "mui-snippets",
  "version": "0.0.0-development",
  "displayName": "MUI Snippets",
  "description": "Code snippets for MUI",
  "icon": "icon.png",
  "publisher": "vscodeshift",
  "engines": {
    "vscode": "^1.51.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "contributes": {
    "configuration": [
      {
        "title": "MUI Snippets",
        "properties": {
          "mui-snippets.importPaths": {
            "type": "string",
            "default": "auto",
            "markdownDescription": "Which import paths to use in generated import statements",
            "enum": [
              "auto",
              "top level",
              "second level"
            ],
            "enumDescriptions": [
              "Uses top level if other top level imports are found, second level otherwise",
              "import { Button } from '@material-ui/core'",
              "import Button from '@material-ui/core/Button'"
            ]
          },
          "mui-snippets.formControlMode": {
            "type": "string",
            "default": "controlled",
            "markdownDescription": "Whether to use controlled or uncontrolled inputs and other form controls in snippets",
            "enum": [
              "controlled",
              "uncontrolled"
            ],
            "enumDescriptions": [
              "Includes value and onChange properties in form controls",
              "Includes defaultValue property in form controls"
            ]
          },
          "mui-snippets.showNotesOnStartup": {
            "type": "boolean",
            "default": true,
            "markdownDescription": "Whether to show release notes on startup"
          }
        }
      }
    ],
