    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.3.7",
    "semantic-release-vsce": "^2.2.8",
    "typescript": "^4.1.3",
    "vscode-test": "^1.2.3"
  },
  "renovate": {
    "extends": [
      ":separateMajorReleases",
      ":combinePatchMinorReleases",
      ":ignoreUnstable",
      ":prImmediately",
      ":renovatePrefix",
      ":updateNotScheduled",
      ":preserveSemverRanges",
      ":semanticPrefixFixDepsChoreOthers",
      ":automergeDisabled",
      "group:monorepos"
    ],
    "automerge": true,
    "major": {
      "automerge": false
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "babel-parse-wild-code": "^1.1.1",
    "jscodeshift": "^0.7.0",
    "react": "^17.0.2",
