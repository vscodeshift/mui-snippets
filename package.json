  "scripts": {
    "lint": "eslint $npm_package_config_lint",
    "lint:fix": "eslint $npm_package_config_lint",
    "lint:watch": "esw --watch $npm_package_config_lint",
    "prettier": "prettier --write .babelrc.js *.json *.md 'src/**/*.{js,ts,tsx}'",
    "prettier:check": "prettier --list-different .babelrc.js *.json *.md 'src/**/*.{js,ts,tsx}'",
    "tsc": "tsc --noEmit",
    "clean": "rimraf out",
    "build": "babel-node --extensions \".js,.ts,.tsx\" scripts/buildReadme.ts && prettier --write README.md && babel src --out-dir out --extensions \".js,.ts,.tsx\"",
    "build:watch": "nodemon -e js,ts,json --watch src --watch .babelrc.js --exec \"(babel-node --extensions '.js,.ts,.tsx' scripts/buildReadme.ts && babel src --out-dir out --extensions '.js,.ts,.tsx'; exit 0)\"",
    "pretest": "npm run build",
    "test": "rimraf .nyc_output; node ./out/test/runTest.js; nyc report; nyc report --reporter=lcov",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov; codecov",
    "vscode:prepublish": "npm run clean && npm run prettier:check && npm run lint && npm run build",
    "open:coverage": "open coverage/lcov-report/index.html",
    "semantic-release": "semantic-release"
  },
  "release": {
    "verifyConditions": [
      "semantic-release-vsce",
      "@semantic-release/github"
    ],
    "prepare": {
      "path": "semantic-release-vsce",
      "packageVsix": "mui-snippets.vsix"
    },
    "publish": [
      "semantic-release-vsce",
      {
        "path": "@semantic-release/github",
        "assets": "mui-snippets.vsix"
      }
    ]
  },
  "config": {
    "lint": "--cache --ext .js,.ts,.tsx src",
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint && npm run tsc",
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@jedwards1211/commitlint-config"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/*.spec.ts",
      "src/**/*.spec.tsx"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vscodeshift/mui-snippets.git"
  },
  "keywords": [
    "vscode",
    "material-ui",
    "snippets"
  ],
  "author": "Andy Edwards",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vscodeshift/mui-snippets/issues"
  },
  "homepage": "https://github.com/vscodeshift/mui-snippets#readme",
